<table>
  <tr>
    <th> Ranking </th>
    <th> Name </th>
    <th> Upvote </th>
  </tr>

  <% objs.each do |medium| %>
    <tr>
      <td>
        <%= medium.rank %>
      </td>
      <td>
        <%= link_to(medium.name, medium) %>
      </td>
      <td>
        <%= render 'shared/upvote_button', obj: medium %>
      </td>
    </tr>
  <% end %>

</table>

<%= button_to "View all Mediums", root_path, method: :get %>
